@model TARge21Shop.Models.Weather.WeatherWievModel

<h1>City</h1>

<div class="container">
    <div class="form-group col-md-3">
        <h2>Forecast for the selected City</h2>

        <label asp-for="Date"></label>
        <span class="badge"@Model.Date></span>

        <label asp-for="EpochDate"></label>
        <span class="badge" @Model.EpochDate></span>

        <label asp-for="Severity"></label>
        <span class="badge" @Model.Severity></span>

        <label asp-for="Text"></label>
        <span class="badge" @Model.Text></span>

        <label asp-for="MobileLink"></label>
        <span class="badge" @Model.MobileLink></span>

        <label asp-for="Link"></label>
        <span class="badge"@Model.Link></span>

        <label asp-for="Category"></label>
        <span class="badge" @Model.Category></span>

        <label asp-for="Temperature.Minimum.Value"></label>
        <span class="badge" @Model.Temperature.Minimum.Value></span>

        <label asp-for="Temperature.Minimum.Unit"></label>
        <span class="badge" @Model.Temperature.Minimum.Unit></span>

        <label asp-for="Temperature.Minimum.UnitType"></label>
        <span class="badge" @Model.Temperature.Minimum.UnitType></span>

        <label asp-for="Temperature.Maximum.Value"></label>
        <span class="badge" @Model.Temperature.Maximum.Value></span>

        <label asp-for="Temperature.Maximum.Unit"></label>
        <span class="badge" @Model.Temperature.Maximum.Unit></span>

        <label asp-for="Temperature.Maximum.UnitType"></label>
        <span class="badge" @Model.Temperature.Maximum.UnitType></span>

        <label asp-for="DayCycle.Icon"></label>
        <span class="badge" @Model.DayCycle.Icon></span>

        <label asp-for="DayCycle.IconPhrase"></label>
        <span class="badge" @Model.DayCycle.IconPhrase></span>

        <label asp-for="DayCycle.HasPrecipitation"></label>
        <span class="badge" @Model.DayCycle.HasPrecipitation></span>

        <label asp-for="DayCycle.PrecipitationType"></label>
        <span class="badge" @Model.DayCycle.PrecipitationType></span>

        <label asp-for="DayCycle.PrecipitationIntensity"></label>
        <span class="badge" @Model.DayCycle.PrecipitationIntensity></span>

        <label asp-for="NightCycle.Icon"></label>
        <span class="badge" @Model.NightCycle.Icon></span>

        <label asp-for="NightCycle.IconPhrase"></label>
        <span class="badge" @Model.NightCycle.IconPhrase></span>

        <label asp-for="NightCycle.HasPrecipitation"></label>
        <span class="badge" @Model.NightCycle.HasPrecipitation></span>

        <label asp-for="NightCycle.PrecipitationType"></label>
        <span class="badge" @Model.NightCycle.PrecipitationType></span>

        <label asp-for="NightCycle.PrecipitationIntensity"></label>
        <span class="badge" @Model.NightCycle.PrecipitationIntensity></span>

    </div>
</div>
